class Solution {
public:
    int tribonacci(int n) {
        if (n <= 2) {
            return n?1:0;
        }

        int prev_n = 1, prev_prev_n = 1, prev_prev_prev_n = 0;
        for(int i=3;i<=n;++i) {
            int cur_n = prev_n + prev_prev_n + prev_prev_prev_n;
            prev_prev_prev_n = prev_prev_n;
            prev_prev_n = prev_n;
            prev_n = cur_n; 
        }
        return prev_n;
    }
};


